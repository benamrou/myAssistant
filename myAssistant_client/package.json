    "license": "Apache",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0 --disableHostCheck",
        "build": "webpack-dev-server --config webpack/webpack.config.js --mode production --base-href /myassistant/ --output-hashing=all",
        "post-build": "node ./src/build/post-build.js",
        "gitbuild": "ng build --prod --base-href /myassistant/master/dist/ ",
        "prod": "ng build --prod --baseHref /myassistant/ --configuration=production",
        "prod_heinens": "ng build --prod --baseHref /myassistant/ --configuration=heinens --output-hashing=all",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "watch": "webpack --watch"
